# Google AI (Gemini) — T2 needs this immediately for P0 spike
GOOGLE_AI_API_KEY=
VERTEX_AI_API_KEY=
GEMINI_MODEL=gemini-3-pro-image-preview
# Anthropic (Claude) — T3 intake/shopping, T0 photo validation
ANTHROPIC_API_KEY=

# Exa (product search) — T3 needs this in P1
EXA_API_KEY=
# EXA_CACHE_DIR=/tmp/exa-cache    # Cache Exa search results (dev/test only)
# LLM_CACHE_DIR=/tmp/llm-cache    # Cache LLM responses: Gemini + Claude (dev/test only)

# Cloudflare R2 (image storage) — T0 sets up in P0
R2_ACCOUNT_ID=
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
R2_BUCKET_NAME=remo-images

# Railway PostgreSQL — T0 sets up in P0
DATABASE_URL=postgresql+asyncpg://user:password@host:5432/remo

# Temporal Cloud — T0 sets up in P0
TEMPORAL_ADDRESS=quickstart-remo-tempo.tmprl.cloud:7233
TEMPORAL_NAMESPACE=quickstart-remo-tempo
TEMPORAL_API_KEY=
TEMPORAL_TASK_QUEUE=remo-tasks

# LangSmith (LLM tracing) — optional, zero-cost when unset
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=remo
LANGSMITH_TRACING=true

# Eval pipeline — "off" (default), "fast" (CLIP/SSIM, $0), "full" (fast + Claude judge, ~$0.02/eval)
EVAL_MODE=off

# App settings (sensible defaults — override only if needed)
ENVIRONMENT=development
LOG_LEVEL=INFO
LOG_FILE=
PRESIGNED_URL_EXPIRY_SECONDS=3600
USE_MOCK_ACTIVITIES=true
USE_TEMPORAL=false
